{
  "expo": {
    "name": "Tayp",
    "slug": "tayp",
    "entryPoint": "./App.tsx",
    "version": "1.0.3",
    "orientation": "portrait",
    "scheme": "tayp",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "description": "Tayp mobile app",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.archie.crawford.tayp",
      "buildNumber": "4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Tayp needs camera access to capture photos and videos.",
        "NSMicrophoneUsageDescription": "Tayp needs microphone access to record audio for videos.",
        "NSPhotoLibraryUsageDescription": "Tayp needs access to your photo library to upload photos and videos.",
        "NSPhotoLibraryAddUsageDescription": "Tayp needs permission to save media to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.archie.crawford.tayp",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "65e2382d-6be8-47b8-b378-e9e81883d23b"
      }
    },
    "owner": "tayp",
    "plugins": [
      "expo-video",
      "expo-font"
    ],
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    }
  }
}
