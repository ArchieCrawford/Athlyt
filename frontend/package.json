{
  "name": "tayp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "prettier \"**/*.{ts,tsx}\" --write",
    "build:ios": "powershell -ExecutionPolicy Bypass -File ./scripts/build.ps1 -Platform ios -Profile production",
    "build:android": "powershell -ExecutionPolicy Bypass -File ./scripts/build.ps1 -Platform android -Profile production",
    "build:all": "powershell -ExecutionPolicy Bypass -File ./scripts/build.ps1 -Platform all -Profile production",
    "bundle:ios": "expo export --platform ios",
    "bundle:android": "expo export --platform android"
  },
